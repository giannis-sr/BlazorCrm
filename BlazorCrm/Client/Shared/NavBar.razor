@inject NavigationManager NavigationManager
<SfAppBar ColorMode="AppBarColor.Primary">
    <SfButton CssClass="e-inherit" IconCss="e-icons e-home" Content="Home"
              OnClick="@(() => NavigateToPage("/"))" />
    <SfButton CssClass="e-inherit" IconCss="e-icons e-people" Content="Employees"
              OnClick="@(() => NavigateToPage("/employees"))" />
    <SfButton CssClass="e-inherit" IconCss="e-icons e-people" Content="Customers"
              OnClick="@(() => NavigateToPage("/customers"))" />
    <SfButton CssClass="e-inherit" IconCss="e-icons e-level-4" Content="Products"
              OnClick="@(() => NavigateToPage("/products"))" />
    <SfButton CssClass="e-inherit" IconCss="e-icons e-calculated-member " Content="Communications"
              OnClick="@(() => NavigateToPage("/communications"))" />
    <SfButton CssClass="e-inherit" IconCss="e-icons e-calculated-member " Content="Notes"
              OnClick="@(() => NavigateToPage("/notes"))" />
    <AppBarSpacer></AppBarSpacer>
    <SfButton CssClass="e-inherit" IconCss="e-icons e-plus " Content="New Employee"
              OnClick="@(() => NavigateToPage("/employees/new"))" />
</SfAppBar>
@code {
   

    void NavigateToPage(string page)
    {
        NavigationManager.NavigateTo(page);
    }
}